    clastext  
    sodcltxt 
  BEGIN
    ACTION_IF FILE_EXISTS_IN_GAME ~%description%.2da~ BEGIN
      COPY_EXISTING ~%description%.2DA~ ~override~
      COUNT_2DA_COLS col_count
      COUNT_2DA_ROWS col_count rows
      FOR (index = 0; index < rows; ++index) BEGIN
        READ_2DA_ENTRY index 0 col_count row_name
//WARLOCK
        PATCH_IF "%row_name%" STRING_EQUAL_CASE "C0WLOCK_FILL" BEGIN
          READ_2DA_ENTRY index 4 col_count warlock_desc
          SPRINT old_warlock ~– Thieving abilities: Pick Pockets.~
          SPRINT new_warlock ~– Thieving abilities: May use Pick Pockets, Find Traps and Detect Illusion.~
          GET_STRREF %warlock_desc% warlock_text
          INNER_PATCH_SAVE warlock_text ~%warlock_text%~ BEGIN
            REPLACE_TEXTUALLY ~%old_warlock%~ ~%new_warlock%~
          END
          INNER_ACTION BEGIN
            STRING_SET_EVALUATE ~%warlock_desc%~ ~%warlock_text%~
          END
		END
		
      END
      BUT_ONLY
    END
  END
  END
  

